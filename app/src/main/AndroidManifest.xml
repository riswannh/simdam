<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.pdam.simdam">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.suin.main.suin.compose.ComposeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.suin.main.suin.konten.KontenActivity" />
        <activity
            android:name=".main.suin.main.suin.inbox.InboxActivity"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".main.suin.main.menu.MenuSuinActivity"
            android:screenOrientation="unspecified">

        </activity>
    </application>

</manifest>